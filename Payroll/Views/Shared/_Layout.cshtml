<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="icon" type="image/png" href="~/Content/img/icon-payroll.png" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" href="~/Content/jquery-ui.css">

    <link rel="stylesheet" href="~/Content/jquery.dataTables.css" />
    <link rel="stylesheet" href="~/Content/autoFill.dataTables.min.css" />
    <link rel="stylesheet" href="~/Content/responsive.dataTables.css" />
    <link rel="stylesheet" href="~/Content/scroller.dataTables.css" />
    <link rel="stylesheet" href="~/Content/jquery-ui.theme.css" />
    <link rel="stylesheet" href="~/Content/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/styles/main.css" />
    <link rel="stylesheet" href="~/Content/animate/animate.css" />

    @RenderSection("scripts", required: false)
</head>
<body>

    <nav class="navbar nav-backcolor navbar-expand-lg">
        <span class="navbar-text text-white">
            <a href="" class="text-white txt-none">
                <h2 class="title-navbar">
                    <i class="fas fa-money-check-alt mr-ico-05 ml-ico-05"></i> IPSNet
                </h2>
            </a>
        </span>
        <form class="form-inline ml-5 invisible">
            <input class="form-control mr-sm-2 form-control-sm" type="search" placeholder="Buscar por empresa" aria-label="Search">
            <button class="btn btn-light my-2 my-sm-0 btn-sm" type="submit">
                <i class="fas fa-search"></i>
            </button>
        </form>
        <form class="form-inline ml-5 invisible">
            <input class="form-control mr-sm-2 form-control-sm" type="search" placeholder="Buscar por empleado" aria-label="Search">
            <button class="btn btn-light my-2 my-sm-0 btn-sm" type="submit">
                <i class="fas fa-search"></i>
            </button>
        </form>
        <form class="form-inline ml-5">
            <div class="text-white font-menu-prin">
                <i class="fas fa-user mr-ico-05"></i> @Session["sUsuario"]
            </div>
            <div class="text-white font-menu-prin btnNameEmpresaSelected pl-3">
                <i class="fas fa-building"></i>
            </div>
            <span class="text-white ml-3 font-menu-prin" id="dateact"></span>
        </form>
        <div class="form-inline ml-5">
            <a href="../Login/Logout" class="btn btn-light btn-sm">
                Salir <i class="fas fa-sign-out-alt ml-ico-05"></i>
            </a>
        </div>
    </nav>

    <div class="body-content">

        @RenderBody()

        <hr />

        <div class="container">
            <footer class="text-center p-4">
                <p class="title-font font-size-footer">
                    <i class="fas fa-copyright mr-05-ico ml-05-ico"></i> @DateTime.Now.Year - GrupoSeri Todos los derechos reservados.
                    <i class="fas fa-flag mr-05-ico ml-05-ico"></i> Icons by <a href="https://fontawesome.com/"> FontAwesome</a>.
                </p>
            </footer>
        </div>

    </div>

    <script src="~/Scripts/jquery-ui.js" charset="UTF-8"></script>
    <script src="~/Scripts/jquery.dataTables.js"></script>
    <script src="~/Scripts/dataTables.autoFill.js"></script>
    <script src="~/Scripts/dataTables.responsive.js"></script>
    <script src="~/Scripts/dataTables.scroller.js"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Scripts/fontawesome/fontawesome.js"></script>
    <script src="~/Scripts/sweetalert/sweetalert.min.js"></script>
    <script src="~/Scripts/datepicker.es.js" charset="UTF-8"></script>
    <script src="~/Scripts/toastr/toastr.min.js"></script>
    

</body>
</html>
